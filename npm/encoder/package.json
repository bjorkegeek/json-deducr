{
  "name": "json-deducr-encoder",
  "version": "1.0.0",
  "description": "JSON Deduplicating Compressed Representation Encoder",
  "homepage": "https://github.com/bjorkegeek/json-deducr",
  "license": "MIT",
  "author": "David BjÃ¶rkevik",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**",
    "src/**/*.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.base.json",
    "test": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjorkegeek/json-deducr.git"
  },
  "bugs": {
    "url": "https://github.com/bjorkegeek/json-deducr/issues"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/object-hash": "^3.0.6",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "object-hash": "^3.0.0"
  }
